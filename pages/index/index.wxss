page{
  background:	#F5F5F5;
  font-family: "微软雅黑";
}
text,view{
  font-size: 32rpx;
}
.s1{
  height: 400rpx;
}
swiper{
  height:400rpx;
}
swiper-item image{
  width: 100%;
  height: 100%;
}
.page-center{
  width: 100%;
  display: flex;
  justify-content: space-around;
  margin: 20rpx 0rpx;
}
.dingwei{
  width: 134rpx;
  height: 72rpx;
  color: #8a8a8a;
  font-size: 30rpx;
  display: flex;
  align-items: center;
  background-color:	#E5E5E5;
  border-radius: 10rpx;
}
.dingwei-tubiao image,.search-tubiao image{
  width: 40rpx;
  height: 40rpx;
  margin: 0 0 5rpx 10rpx;
}
.search{
  width: 550rpx;
  height: 70rpx;
  color: #8a8a8a;
  border: 1rpx solid #8a8a8a;
  display: flex;
  align-items: center;
  border-radius: 10rpx;
}
.jiudian{
  height: 220rpx;
  background: #fff;
  display: flex;
  margin: 0 auto;
  padding: 15rpx 15rpx;
  margin-bottom: 20rpx;
}
.jiudian-image{
  width: 280rpx;
  height: 220rpx;
  border-radius: 10rpx;
}
.jiudian-telte{
  margin-left: 12rpx;
}
.xingxing{
  display: inline-flex;
  margin-left: 9rpx;
}
.xingxing image{
  width: 45rpx;
  height: 45rpx;
}
.jiuli{
  padding:15rpx 0rpx 15rpx 250rpx;
  color: #FF7F00;
}
.jiudian-dingwei{
  display: flex;
  align-items: center;
  color: #515151;
}
